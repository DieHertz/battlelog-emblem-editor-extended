{
    "manifest_version": 2,

    "name": "Battlelog Emblem Editor Extended",
    "description": "Adds some useful functionality for emblem editor.",
    "version": "0.4.5.5",

    "permissions": [
        "http://battlelog.battlefield.com/*"
    ],
    "icons": {
        "128": "icon_128x128.png",
        "64": "icon_64x64.png"
    },

    "content_scripts": [{
        "matches": [ "http://battlelog.battlefield.com/*" ],
        "js": [ "actions.js", "controls.js", "main.js", "inject.js" ],
        "css": [ "styles.css" ],
        "run_at": "document_end"
    }]
}
